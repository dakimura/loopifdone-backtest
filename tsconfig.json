{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "sourceMap": true,
    "types": [
      "node",
      "jest"
    ],
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": false,
    "alwaysStrict": true,
    "strict": true,
    "skipLibCheck": true,
    "pretty": true,
    "newLine": "lf",
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}